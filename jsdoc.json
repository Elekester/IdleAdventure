{
	"opts": {
		"destination": "docs/",
		"template": "C:/Program Files/nodejs/node_modules/npm/node_modules/jaguarjs-jsdoc"
	},
	"plugins": ["plugins/markdown"],
	"recurseDepth": 10,
	"source": {
		"include": ["main.js", "README.md"]
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"]
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"applicationName": "Idle Adventure",
		"meta": {
			"title": "Idle Adventure Documentation"
		},
		"linenums": true
	}
}

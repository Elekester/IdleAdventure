{
	"opts": {
		"destination": "../docs/",
		"template": "C:/Program Files/nodejs/node_modules/npm/node_modules/better-docs"
	},
	"plugins": ["plugins/markdown"],
	"recurseDepth": 10,
	"source": {
		"include": ["../main.js", "../README.md"]
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"]
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"applicationName": "Idle Adventure",
		"search": true,
		"better-docs": {
			"name": "Idle Adventure Documentation",
			"title": "Idle Adventure - ",
			"css": "docsStyle.css",
			"navLinks": [ {
				"label": "Idle Adventure",
				"href": "../index.html"
			}, {
				"label": "GitHub Repository",
				"href": "https://github.com/Elekester/IdleAdventure"
			}]
		}
	}
}
